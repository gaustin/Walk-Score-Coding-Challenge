#!/usr/bin/python

import sys
import fileinput

import os
sys.path.insert(0, os.path.abspath('.'))

from digraphing import Digraph

graph = Digraph()

for line in fileinput.input():
    pair = line.strip().split("\t")

    graph.addArc(pair[0], pair[1])

compressedGraph = graph.compress()

for node, outArcs in compressedGraph.graph.items():
    for arcTo in outArcs:
        print "%s\t%s" % (node, arcTo)

